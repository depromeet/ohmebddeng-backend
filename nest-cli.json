{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "plugins": [
      {
        "name": "@nestjs/swagger",
        "options": {
          "classValidatorShim": true,
          "dtoKeyOfComment": true,
          "controllerKeyOfComment": true
        }
      }
    ]
  }
}
